<body>
    <div class="text text1"> Let's Chat </div><br>
    <div class="text text2">to be Connected!</div>
    <div class="container">
        <button type="button" class="btn btn-style" data-toggle="modal" data-target="#myModal">
            Click Me
        </button>
        <!-- The Modal -->
        <div class="modal fade mt-10" id="myModal">
            <div class="modal-dialog">
                <div class="modal-content ">
                    <!-- Modal Header -->
                    <div class="modal-header ">
                        <h1 class="modal-title">Welcome Users!</h1>
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                    </div>

                    <!-- Modal body -->
                    <div class="modal-body">
                        <form>
                            <div class="form-group">
                                <h5 class="font-weight-bold text-primary" for="nickname">Nickname:</h5>
                                <input type="text" class="form-control" id="username"
                                    placeholder="Enter your amazing Nickname">
                            </div>
                        </form>
                    </div>

                    <!-- Modal footer -->
                    <div class="modal-footer">
                        <a type="button" id="sub" class="btn btn-danger">Connect</a>
                    </div>

                </div>
            </div>
        </div>

    </div>
